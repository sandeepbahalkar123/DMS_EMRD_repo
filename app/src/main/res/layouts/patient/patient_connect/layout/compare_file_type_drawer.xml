<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <!-- for below scrollview android:layout_height="@dimen/dp210"-->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/SkyBlue"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp16"
                    android:background="@color/SkyBlue"
                    android:orientation="horizontal"
                    android:padding="@dimen/dp5">

                    <android.support.v7.widget.AppCompatImageButton
                        android:id="@+id/imageCloseDrawer"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp10"
                        android:background="@drawable/ic_close_white"
                        android:textColor="@color/white" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="4"
                        android:gravity="center"
                        android:text="@string/medical_records"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp18"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>

            <TableRow
                android:id="@+id/rowScrollBoth"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp5"
                android:orientation="horizontal"
                android:visibility="gone"
                android:weightSum="6">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="4"
                    android:gravity="center_vertical"
                    android:text="@string/scrollingBoth"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp13"
                    android:textStyle="bold" />

                <Switch
                    android:id="@+id/et_uhid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="2"
                    android:checked="false"
                    android:maxLines="1"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp13"
                    android:textStyle="bold" />


            </TableRow>


            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dp5"
                android:orientation="vertical"
                android:weightSum="6">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:maxLines="1"
                    android:text="@string/patientInformation"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp13"
                    android:textStyle="bold" />


            </TableRow>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp1"
                android:layout_marginBottom="@dimen/margin_tiny"
                android:background="@color/grey_500"
                android:visibility="gone" />


            <TextView
                android:id="@+id/tvPatientName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_tiny"
                android:layout_marginLeft="@dimen/dp5"
                android:layout_marginStart="@dimen/dp5"
                android:layout_marginTop="@dimen/margin_tiny"
                android:drawableLeft="@drawable/usercheck"
                android:drawablePadding="@dimen/dp5"
                android:drawableStart="@drawable/usercheck"
                android:gravity="center_vertical"
                android:padding="@dimen/dp2"
                android:textColor="@color/black"
                android:textSize="@dimen/sp13"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_tiny"
                    android:layout_marginLeft="@dimen/dp5"
                    android:layout_marginStart="@dimen/dp5"
                    android:layout_weight="0.4"
                    android:drawableLeft="@drawable/keyblack"
                    android:drawablePadding="@dimen/dp5"
                    android:drawableStart="@drawable/keyblack"
                    android:gravity="center"
                    android:padding="@dimen/dp2"
                    android:text="@string/uhid"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp13"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.6">

                    <TextView
                        android:id="@+id/tvPatientUHID"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_tiny"
                        android:layout_marginLeft="@dimen/dp5"
                        android:layout_marginStart="@dimen/dp5"
                        android:background="@drawable/shape_rounded_corners_with_border"
                        android:gravity="center_vertical"
                        android:padding="@dimen/dp5"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp13"
                        tools:text="@string/uhid" />

                </LinearLayout>


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="2">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_tiny"
                    android:layout_marginLeft="@dimen/dp5"
                    android:layout_marginStart="@dimen/dp5"
                    android:layout_weight="0.5"
                    android:drawableLeft="@drawable/placeholderblack"
                    android:drawablePadding="@dimen/dp5"
                    android:drawableStart="@drawable/placeholderblack"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dp2"
                    android:text="@string/address"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp13"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvPatientLocation"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_tiny"
                    android:layout_marginLeft="@dimen/dp5"
                    android:layout_marginStart="@dimen/dp5"
                    android:layout_weight="1.5"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dp2"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp13"
                    tools:text="@string/address" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">


                <LinearLayout
                    android:id="@+id/fileOneLay"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <View
